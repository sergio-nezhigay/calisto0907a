/* Responsive */

@media only screen and (max-width: 992px) {
  .bundle-wrapper .header__body {
        flex-direction: column;
        padding-bottom: 0;
    }
    .bundle-wrapper .product_indicator_wrapper {
        margin-bottom: 15px;
        justify-content: center;
        padding:0 10px;
    }
    .m-breadcrumb--wrapper {
        margin: 0;
    }
    .bundle-wrapper .material__wrapper .material__wrapper__container, 
    .bundle-wrapper .shipping__wrapper .shipping__wrapper__container {
        margin-bottom: 20px;
    }
    .bundle-wrapper .heading__discount__text {
        padding: 6px 0;
        width: 100%;
    }
}

@media only screen and (min-width: 768px) {
  .desktop-hide {
    display: none;
  }
  
}
@media only screen and (max-width: 767px) {
  .mobile-hide {
    display: none;
  }
  .bundle-wrapper .shipping__wrapper {
      border-bottom: none;
  }
  .bundle-wrapper .main-product__block .m-product-title, .bundle-wrapper .main-product__block-price span, .bundle-wrapper .m-price__sale .m-price-item--regular {
    font-size: 20px;
  }
  .bundle-wrapper .main-product__block-buy_buttons button {
        font-size: 20px;
  }
}


@media only screen and (max-width: 600px) {
   .bundle-wrapper .header__body {
        flex-direction: column;
        padding: 0;
        padding-top: 10px;
        background-color: #15ABBA0D;
    }
    .bundle-wrapper .header__body .main_title {
        font-size: 20px;
        text-align: center;
    }
    .bundle-wrapper .heading__discount__text {
        padding: 6px 0;
        width: 100%;
    }
    .bundle-wrapper .heading__discount__text h4 {
        font-size: 12px !important;
    }
    .bundle-wrapper .m-section-my {
        padding: 0;
    }
    .bundle-wrapper .m-main-product--wrapper {
        padding: 0 15px;
        margin-bottom: 25px;
    }
    .bundle-wrapper .product_indicator_wrapper {
        margin-top: 10px;
        background-color: #15ABBA0D;
        padding: 10px 10px;
        border-top: none;
    }
    .bundle-wrapper .header__body .product_indicator_wrapper {
      background-color: transparent;
    }
    .bundle-wrapper .product_indicator_wrapper .indicator_icon {
        margin-right: 10px;
        width: 35px;
    }
    
    /* Products CSS  */
    .bundle-wrapper .m-product-info-content, .bundle-wrapper .m-product-option:not(:last-child) {
        padding-bottom: 15px;
    }
    .bundle-wrapper .material__wrapper, .bundle-wrapper .shipping__wrapper {
        margin-bottom: 10px;
        padding: 15px 10px;
    }
    .bundle-wrapper .m-product-option {
        margin-bottom: 15px;
    }

    .bundle-wrapper .m-product-option:last-of-type {
        border-bottom: 0;
        margin-bottom: 0;
        padding-bottom: 0;
    }

    .bundle-wrapper .footer__body .footer__discount__text {
        font-weight: 700;
        font-size: 14px;
        margin-bottom: 5px;
        padding: 7px;
    }

    .bundle-wrapper .product_bundle_price_wrapper .bundle__total_price span.total_price_title {
        font-weight: 700;
        font-size: 14px;
        display: block;
    }
    
    .bundle-wrapper .product_bundle_price_wrapper .bundle__total_price span.total_price {
        font-size: 24px;
        font-weight: 700;
    }

    .bundle-wrapper .bundle__total_save_price {
        margin-left: 10px;
        margin-bottom: 5px;
    }
    
    .bundle-wrapper .bundle__total_compare_price span {
        font-size: 24px;
    }
    
    .bundle-wrapper .product_bundle_price_wrapper {
        align-items: end;
        justify-content: start;
      gap:0;
    }

    .bundle-wrapper .main-product__block-buy_buttons button {
        margin: 10px 0 25px;
    }
    
}

@media only screen and (max-width: 390px) {
    .bundle-wrapper .main-product__block .m-product-title, .bundle-wrapper .main-product__block-price span, .bundle-wrapper .m-price__sale .m-price-item--regular {
      font-size: 16px;
  }
}
